{
  "name": "@liam-hq/erd-web",
  "private": true,
  "version": "0.1.0",
  "dependencies": {
    "@liam-hq/db": "workspace:*",
    "@liam-hq/db-structure": "workspace:*",
    "@liam-hq/erd-core": "workspace:*",
    "@liam-hq/ui": "workspace:*",
    "@next/third-parties": "15.1.5",
    "@octokit/auth-app": "6.1.3",
    "@octokit/rest": "20.0.2",
    "@sentry/nextjs": "8",
    "cheerio": "1.0.0",
    "date-fns": "4.1.0",
    "next": "15.1.2",
    "react": "18.3.1",
    "react-dom": "18",
    "valibot": "^1.0.0-beta.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@types/gtag.js": "0.0.20",
    "@types/node": "22.9.0",
    "@types/react": "18",
    "@types/react-dom": "18",
    "typed-css-modules": "0.9.1",
    "typescript": "5"
  },
  "scripts": {
    "build": "pnpm gen && next build",
    "dev": "pnpm run '/^dev:.*/'",
    "dev:css": "tcm components --watch",
    "dev:next": "next dev --port 3001",
    "fmt": "pnpm run '/^fmt:.*/'",
    "fmt:biome": "biome check --write --unsafe .",
    "gen": "pnpm run '/^gen:.*/'",
    "gen:css": "tcm components",
    "lint": "pnpm run '/^lint:.*/'",
    "lint:biome": "biome check .",
    "postinstall": "cp ../../packages/db-structure/node_modules/@ruby/prism/src/prism.wasm prism.wasm",
    "start": "next start"
  }
}
