{
  "name": "migration-web",
  "private": true,
  "version": "0.1.0",
  "dependencies": {
    "@langchain/community": "0.3.33",
    "@langchain/core": "0.3.42",
    "@langchain/openai": "0.4.4",
    "@prisma/client": "6.4.1",
    "@supabase/supabase-js": "2.49.1",
    "html-to-text": "9.0.5",
    "langchain": "0.3.19",
    "langfuse-langchain": "3.36.0",
    "next": "15.1.2",
    "prisma": "6.4.1",
    "react": "18.3.1",
    "react-dom": "18"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@types/node": "22.9.0",
    "@types/react": "18",
    "@types/react-dom": "18",
    "typed-css-modules": "0.9.1",
    "typescript": "5"
  },
  "scripts": {
    "build": "pnpm gen && next build",
    "dev": "pnpm run '/^dev:.*/'",
    "dev:css": "tcm components --watch",
    "dev:next": "next dev --port 3003",
    "fmt": "pnpm run '/^fmt:.*/'",
    "fmt:biome": "biome check --write --unsafe .",
    "gen": "pnpm run '/^gen:.*/'",
    "gen:css": "tcm components",
    "lint": "pnpm run '/^lint:.*/'",
    "lint:biome": "biome check .",
    "start": "next start",
    "supabase:pull": "pnpm supabase db pull",
    "supabase:reset": "pnpm supabase db reset",
    "supabase:start": "pnpm supabase start",
    "supabase:stop": "pnpm supabase stop"
  }
}
